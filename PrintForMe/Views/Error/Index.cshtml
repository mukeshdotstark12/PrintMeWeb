
@using CMS.Helpers
@model System.Web.Mvc.HandleErrorInfo
@{
    ViewBag.Title = "Error";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="thank-you-page">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="thank-you-content">
                    <h2>@ResHelper.GetString("PrintForMe.ApplicationError")</h2>
                    <p>@ResHelper.GetString("PrintForMe.ErrorMessage")</p>
                    <h6>@Html.ActionLink(@ResHelper.GetString("PrintForMe.GotoHome"), "Index", "Home")</h6>
                </div>
            </div>
        </div>
    </div>
</div>